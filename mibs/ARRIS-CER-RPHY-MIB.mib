    SYNTAX      CerRpdAdpLldpPortStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry containing status information
         applicable to ADP and LLDP on a particular RPD."
    INDEX { cerRpdAdpLldpPortStatusRpdIndex,
            cerRpdAdpLldpPortStatusEnetPortNum }
    ::= { cerRpdAdpLldpPortStatusTable 1 }

CerRpdAdpLldpPortStatusEntry ::=
    SEQUENCE {
        cerRpdAdpLldpPortStatusRpdIndex           CerRpdIndex,
        cerRpdAdpLldpPortStatusEnetPortNum        Unsigned32,
        cerRpdAdpLldpPortStatusAdpState           INTEGER,
        cerRpdAdpLldpPortStatusAdpHopCnt          Unsigned32,
        cerRpdAdpLldpPortStatusNbrMacAddr         MacAddress,
        cerRpdAdpLldpPortStatusChassisSubType     INTEGER,
        cerRpdAdpLldpPortStatusChassisId          OCTET STRING,
        cerRpdAdpLldpPortStatusPortSubType        INTEGER,
        cerRpdAdpLldpPortStatusPortId             OCTET STRING
    }

cerRpdAdpLldpPortStatusRpdIndex OBJECT-TYPE
    SYNTAX      CerRpdIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The RPD ifIndex of the Ethernet port."
    ::= { cerRpdAdpLldpPortStatusEntry 1 }

cerRpdAdpLldpPortStatusEnetPortNum OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The Ethernet port number on the RPD."
    ::= { cerRpdAdpLldpPortStatusEntry 2 }

cerRpdAdpLldpPortStatusAdpState OBJECT-TYPE
    SYNTAX      INTEGER {
        unknown (0),
        initialized (1),
        holddown (2),
        waitfornbr (3),
        uplink (4),
        downlink (5)
    }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The current state of the ADP state machine for the Ethernet port on the RPD.
         This is populated using vendor-specific TLV 21.4.8.2"
    ::= { cerRpdAdpLldpPortStatusEntry 3 }

cerRpdAdpLldpPortStatusAdpHopCnt OBJECT-TYPE
    SYNTAX      Unsigned32 (0..255)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The current value of the ADP Hop Count parameter for the Ethernet port on the RPD.
         This is populated using vendor-specific TLV 21.4.8.3"
    ::= { cerRpdAdpLldpPortStatusEntry 4 }

cerRpdAdpLldpPortStatusNbrMacAddr OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The MAC Address sent by the neighboring node via LLDP.
         This is populated using vendor-specific TLV 21.5.2"
    ::= { cerRpdAdpLldpPortStatusEntry 5 }
    
cerRpdAdpLldpPortStatusChassisSubType OBJECT-TYPE
    SYNTAX      INTEGER {
        unknown (0),
        chassisComponent (1),
        interfaceAlias (2),
        portComponent (3),
        macAddress (4),
        networkAddress (5),
        interfaceName (6),
        locallyAssigned (7)
    }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The Chassis Subtype identification sent by the neighboring node.
         This is populated using vendor-specific TLV 21.5.3.
         The RPD reports macAddress(4) as its Chassis Subtype.
         If a connected CIN device supports LLDP, it may report one of
         the other Chassis Subtype values as defined by 802.1AB."
    ::= { cerRpdAdpLldpPortStatusEntry 6 }

cerRpdAdpLldpPortStatusChassisId OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..255))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The Chassis ID value sent by the neighboring node.
         This is populated using vendor-specific TLV 21.5.4.
         For an RPD, this attribute contains a MAC address."
    ::= { cerRpdAdpLldpPortStatusEntry 7 }

cerRpdAdpLldpPortStatusPortSubType OBJECT-TYPE
    SYNTAX      INTEGER {
        unknown (0),
        interfaceAlias (1),
        portComponent (2),
        macAddress (3),
        networkAddress (4),
        interfaceName (5),
        agentCircuitId (6),
        locallyAssigned (7)
    }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The Port Subtype identification sent by the neighboring node.
         This is populated using vendor-specific TLV 21.5.5.
         The RPD reports locallyAssigned(7) as its Port Subtype.
         If a connected CIN device supports LLDP, it may report one of
         the other Port Subtype values as defined by 802.1AB."
    ::= { cerRpdAdpLldpPortStatusEntry 8 }

cerRpdAdpLldpPortStatusPortId OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..255))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The Port ID value sent by the neighboring node.
         This is populated using vendor-specific TLV 21.5.6.
         For an RPD, this attribute contains an Ethernet port number."
    ::= { cerRpdAdpLldpPortStatusEntry 9 }

