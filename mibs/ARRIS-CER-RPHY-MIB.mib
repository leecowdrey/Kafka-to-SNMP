cerRpdUsScQamQueryControlGroup OBJECT IDENTIFIER ::= {cerRphyMib 77}

    cerRpdUsScQamQueryControlRpdIndex OBJECT-TYPE
        SYNTAX CerRpdIndex
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Global index for this RPD."
        ::= { cerRpdUsScQamQueryControlGroup 1 }

    cerRpdUsScQamQueryControlModIndex OBJECT-TYPE
        SYNTAX Integer32 (1..2147483647)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Index of the SC-QAM modulation profile whose parameters are sent to the RPD in TLV 150 UsScQamProfileQuery."
        ::= { cerRpdUsScQamQueryControlGroup 2 }

    cerRpdUsScQamQueryControlEnable OBJECT-TYPE
        SYNTAX TruthValue
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION 
            "Setting this attribute to true causes the CMTS to begin the process of sending TLV 150 
             UsScQamProfileQuery and 151 UsScQamProfileResponse to the RPD."
        ::= { cerRpdUsScQamQueryControlGroup 3 }

cerRpdUsScQamIucResponseTable OBJECT-TYPE
         SYNTAX SEQUENCE OF CerRpdUsScQamIucResponseEntry 
         MAX-ACCESS not-accessible
         STATUS current
         DESCRIPTION
             "This table stores what an RPD sent in TLV 151.2 which are recommended parameters per IUC.
              The superstring returned in TLV 151.1 is stored in cerRpdUsScQamPreambleResponseSuperstring.
              The RPD that sent TLV 151 is stored in cerRpdUsScQamSuperstringResponseRpdIndex. The SC-QAM 
              modulation profile used to populate TLV 150 is stored in cerRpdUsScQamSuperstringResponseModIndex.
              Only one response is stored in this table at a time; new responses overwrite existing responses."
        ::= { cerRphyMib 78 }

cerRpdUsScQamIucResponseEntry OBJECT-TYPE
        SYNTAX CerRpdUsScQamIucResponseEntry 
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "This table stores what an RPD sent in TLV 151.2 which are recommended parameters per IUC.
             The values returned in TLV 151 that are not per IUC are stored in MIB variables that are 
             part of cerRpdUsScQamSuperstringResponseGroup. The RPD and SC-QAM modulation profile related
             to TLV 151 are also stored in MIB variables in cerRpdUsScQamSuperstringResponseGroup. 
             Only one response is stored in this table at a time; new responses overwrite existing responses."
        INDEX { cerRpdUsScQamIucResponseIucIndex}
        ::= { cerRpdUsScQamIucResponseTable 1 }

CerRpdUsScQamIucResponseEntry ::= SEQUENCE {
         cerRpdUsScQamIucResponseIucIndex INTEGER,
         cerRpdUsScQamIucResponseLength Integer32,
         cerRpdUsScQamIucResponseOffset Integer32,
         cerRpdUsScQamIucResponseModType INTEGER,
         cerRpdUsScQamIucResponseScramblerSeed Integer32,
         cerRpdUsScQamIucResponseGuardTime Unsigned32,
         cerRpdUsScQamIucResponseRpdIndex CerRpdIndex
    }

cerRpdUsScQamIucResponseIucIndex OBJECT-TYPE
         SYNTAX INTEGER {
                 request(1),
                 requestData(2),
                 initialRanging(3),
                 periodicRanging(4),
                 shortData(5),
                 longData(6),
                 advPhyShortData(9),
                 advPhyLongData(10),
                 ugs(11)
         }
         MAX-ACCESS not-accessible
         STATUS current
         DESCRIPTION
             "US SC-QAM IUC related to this subset of RPD recommended preamble settings, from TLV 151.2.1 ResponseScQamCode."
         ::= { cerRpdUsScQamIucResponseEntry 1 }

cerRpdUsScQamIucResponseLength OBJECT-TYPE
         SYNTAX Integer32 (0..1536)
         MAX-ACCESS read-only
         STATUS current
         DESCRIPTION
                 "The RPD's recommended preamble length for this IUC in bits, from TLV 151.2.2 ResponseScQamPreambleLen."
         DEFVAL { 0 }
         ::= { cerRpdUsScQamIucResponseEntry 2 }

cerRpdUsScQamIucResponseOffset OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The RPD's recommended offset into the preamble superstring for this IUC, from TLV 151.2.3 
             ResponseScQamPreambleOffset."
         DEFVAL { 0 }
         ::= { cerRpdUsScQamIucResponseEntry 3 }

cerRpdUsScQamIucResponseModType OBJECT-TYPE
        SYNTAX INTEGER {
                 qpsk0(1),
                 qpsk1(2)
         }
         MAX-ACCESS read-only
         STATUS current
         DESCRIPTION
             "The RPD's recommended preamble type, from TLV 151.2.4 ResponseScQamPreambleModType."
         DEFVAL { qpsk0 }
         ::= { cerRpdUsScQamIucResponseEntry 4 }

cerRpdUsScQamIucResponseScramblerSeed OBJECT-TYPE
         SYNTAX Integer32 (0..32767)
         MAX-ACCESS read-only
         STATUS current
         DESCRIPTION
             "The RPD's recommended value for the upstream scrambler seed in the RPD, from TLV 151.2.5 
              ResponseScQamScramblerSeed. The leftmost 15 bytes in the 2 byte field are used."
         DEFVAL { 0 }
         ::= { cerRpdUsScQamIucResponseEntry 5 }

cerRpdUsScQamIucResponseGuardTime OBJECT-TYPE
        SYNTAX Unsigned32 (8..96)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
             "The RPD's recommended value for the number of symbol-times which must follow the end of
              this channel's burst, from TLV 151.2.6 ResponseScQamGuardTime. A value of 0 is only valid
              for SCDMA modulation profiles, which are not supported."
        DEFVAL { 8 }
        ::= { cerRpdUsScQamIucResponseEntry 6 }

cerRpdUsScQamIucResponseRpdIndex OBJECT-TYPE
         SYNTAX CerRpdIndex
         MAX-ACCESS read-only
         STATUS current
         DESCRIPTION
              "Global index for the RPD that sent the most recent SC-QAM US preamble response in TLV 151."
         ::= { cerRpdUsScQamIucResponseEntry 7 }

cerRpdUsScQamSuperstringResponseGroup OBJECT IDENTIFIER ::= { cerRphyMib 79 }

    cerRpdUsScQamSuperstringResponseRpdIndex OBJECT-TYPE
         SYNTAX CerRpdIndex
         MAX-ACCESS read-only
         STATUS current
         DESCRIPTION
                  "Global index for the RPD that sent the most recent SC-QAM US preamble response in TLV 151."
         ::= { cerRpdUsScQamSuperstringResponseGroup 1 }

    cerRpdUsScQamPreambleResponseSuperstring OBJECT-TYPE
         SYNTAX      OCTET STRING (SIZE(0..192)) 
         MAX-ACCESS read-only
         STATUS      current
         DESCRIPTION
            "The RPD's recommended preamble superstring for the SC-QAM UCD, sent to the Core in TLV 151.1 
             ResponseScQamPreambleString. This also applies to the data per IUC from TLV 151 stored in 
             cerRpdUsScQamIucResponseTable."
         DEFVAL { ''h }
         ::= { cerRpdUsScQamSuperstringResponseGroup 2 }

    cerRpdUsScQamPreambleResponseTimestamp OBJECT-TYPE
        SYNTAX DateAndTime
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The date and time that TLV 151 UsScQamProfileResponse was received from this RPD. 
             This also applies to the data per IUC from TLV 151 stored in cerRpdUsScQamIucResponseTable."
        DEFVAL { '0000000000000000'h }
        ::= { cerRpdUsScQamSuperstringResponseGroup 3 }


cerRpdUsOfdmaQueryControlGroup OBJECT IDENTIFIER ::= {cerRphyMib 80} 

   cerRpdUsOfdmaQueryControlRpdIndex OBJECT-TYPE
