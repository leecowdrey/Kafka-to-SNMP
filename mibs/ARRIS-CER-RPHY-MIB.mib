    DEFVAL { 0 }
    ::= { cerRpdSsdEntry 8}

cerRpdSsdCosignerCvcIndex OBJECT-TYPE
    SYNTAX  	Integer32 (0|1..255)
    MAX-ACCESS read-create
    STATUS  	current
    DESCRIPTION
        "The index into the cerRpdSsdCvcTable for the cosigner CVC. Zero if no cosigner."
    DEFVAL { 0 }
    ::= { cerRpdSsdEntry 9 }

cerRpdSsdEnabled OBJECT-TYPE
