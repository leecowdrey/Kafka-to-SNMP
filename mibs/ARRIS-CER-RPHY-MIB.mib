    SYNTAX INTEGER {
            tftp(1),
            http(2)
    }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION
        "The file transport method for the software download." 
    DEFVAL { tftp }
    ::= {cerRpdSsdEntry 7}

cerRpdSsdManufCvcIndex OBJECT-TYPE
    SYNTAX    Integer32 (0..255)
    MAX-ACCESS read-create
