cerL2Tpv3TunnelInfoRpdCCID OBJECT-TYPE
    SYNTAX          Unsigned32 (0..4294967295)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "The RPD L2TPv3 CCID of the tunnel. Zero means not known." 
    ::= { cerL2Tpv3TunnelInfoEntry 3 }

cerL2Tpv3TunnelInfoIpAddrType OBJECT-TYPE
    SYNTAX InetAddressType
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Type of the tunnel IP address (IPv4, IPv6)."
    ::= { cerL2Tpv3TunnelInfoEntry 4 }

cerL2Tpv3TunnelInfoRpdLcceIpAddr OBJECT-TYPE
    SYNTAX InetAddressIPv4or6
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The tunnel IP address at the RPD."
    ::= { cerL2Tpv3TunnelInfoEntry 5 }

