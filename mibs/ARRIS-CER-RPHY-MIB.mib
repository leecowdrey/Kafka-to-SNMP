--------------------------------------------------------------------------------------------------
cerRpdSsdCvcTable OBJECT-TYPE
    SYNTAX SEQUENCE OF CerRpdSsdCvcEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table contains the R-PHY manufacturer and optionally MSO co-signer
         Code Verification Certificates (CVCs) and issuing CVC CA certificates.
         These certificates are passed to the RPDs to enable verification of 
         Secure Software Download (SSD) images."
    ::= { cerRphyMib 61 }

cerRpdSsdCvcEntry OBJECT-TYPE
    SYNTAX CerRpdSsdCvcEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "An entry for a manufacturer or co-signer CVC and it's issuing CVC-CA 
         certificate pair."
    INDEX { cerRpdSsdCvcIndex }
    ::= { cerRpdSsdCvcTable 1}

CerRpdSsdCvcEntry ::= SEQUENCE {
    cerRpdSsdCvcIndex             Integer32,
    cerRpdSsdCvcDescription       DisplayString,
    cerRpdSsdCvcSubject           SnmpAdminString,
    cerRpdSsdCvcIssuer            SnmpAdminString,
    cerRpdSsdCvcSerialNumber      SnmpAdminString,
    cerRpdSsdCvcCert              X509Certificate,
    cerRpdSsdCvcCaSubject         SnmpAdminString,
    cerRpdSsdCvcCaIssuer          SnmpAdminString,
    cerRpdSsdCvcCaSerialNumber    SnmpAdminString,
    cerRpdSsdCvcCaCert            X509Certificate,
    cerRpdSsdCvcStatus            RowStatus
}

cerRpdSsdCvcIndex OBJECT-TYPE
    SYNTAX           Integer32 (1..255)
    MAX-ACCESS       not-accessible
    STATUS           current
    DESCRIPTION
        "The type of a CCAP certificate."
    ::= { cerRpdSsdCvcEntry 1 }

cerRpdSsdCvcDescription OBJECT-TYPE
    SYNTAX           DisplayString
    MAX-ACCESS       read-create
    STATUS           current
    DESCRIPTION
        "An optional text description of the CVC/CVC-CA cert. "                 
    ::= { cerRpdSsdCvcEntry 2 }

cerRpdSsdCvcSubject OBJECT-TYPE
    SYNTAX           SnmpAdminString
    MAX-ACCESS       read-only
    STATUS           current
    DESCRIPTION
        "The subject name encoded in the x509 certificate,
         formatted as: /C=<country>/O=<Company Name>/CN=<FQDN> "                 
    ::= { cerRpdSsdCvcEntry 3 }

cerRpdSsdCvcIssuer OBJECT-TYPE
    SYNTAX           SnmpAdminString
    MAX-ACCESS       read-only
    STATUS           current
    DESCRIPTION
        "The subject name encoded in the x509 certificate,
         formatted as: /C=<country>/O=<Company Name>/OU=<CA identifier>/CN=<FQDN> "
    ::= { cerRpdSsdCvcEntry 4 }

cerRpdSsdCvcSerialNumber OBJECT-TYPE
    SYNTAX            SnmpAdminString
    MAX-ACCESS        read-only
    STATUS            current
    DESCRIPTION
        "This certificate's serial number represented as an asci string."
    ::= { cerRpdSsdCvcEntry 5 }

cerRpdSsdCvcCert  OBJECT-TYPE
    SYNTAX            X509Certificate
    MAX-ACCESS        read-create
    STATUS            current
    DESCRIPTION
        "The X509 DER-encoded CVC."    
    ::= { cerRpdSsdCvcEntry 6 }

cerRpdSsdCvcCaSubject OBJECT-TYPE
    SYNTAX           SnmpAdminString
    MAX-ACCESS       read-only
    STATUS           current
    DESCRIPTION
        "The subject name encoded in the x509 certificate,
         formatted as: /C=<country>/O=<Company Name>/OU=<CA identifier>/CN=<CA Name> "                 
    ::= { cerRpdSsdCvcEntry 7 }

cerRpdSsdCvcCaIssuer OBJECT-TYPE
    SYNTAX           SnmpAdminString
    MAX-ACCESS       read-only
    STATUS           current
    DESCRIPTION
        "The subject name encoded in the x509 certificate,
         formatted as: /C=<country>/O=<Company Name>/OU=<CA identifier>/CN=<CA Name> "
    ::= { cerRpdSsdCvcEntry 8 }

cerRpdSsdCvcCaSerialNumber OBJECT-TYPE
    SYNTAX            SnmpAdminString
    MAX-ACCESS        read-only
    STATUS            current
    DESCRIPTION
        "This CA certificate's serial number represented as an ascii string."
    ::= { cerRpdSsdCvcEntry 9 }

cerRpdSsdCvcCaCert OBJECT-TYPE
    SYNTAX            X509Certificate
    MAX-ACCESS        read-create
    STATUS            current
    DESCRIPTION
        "The X509 DER-encoded CVC CA certificate."    
    ::= { cerRpdSsdCvcEntry 10 }

cerRpdSsdCvcStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The RowStatus for controlling entries in this table. 
         Allowed values are createAndGo, destroy, and active."
    ::= { cerRpdSsdCvcEntry 11 }

