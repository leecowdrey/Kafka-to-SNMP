         DEFVAL { 0 }
         ::= { cerRpdUsOfdmaPreambleResponseGroup 8 }

   cerRpdUsOfdmaPreambleResponseSuperstring OBJECT-TYPE
        SYNTAX      OCTET STRING (SIZE(0..192)) 
        MAX-ACCESS read-only
        STATUS      current
        DESCRIPTION
            "The RPD's recommended preamble superstring for the OFDMA UCD, 
             sent in TLV 153.3 ResponseOfdmaPreambleString"
        DEFVAL { ''h }
        ::= { cerRpdUsOfdmaPreambleResponseGroup 9 }

   cerRpdUsOfdmaPreambleResponseTimestamp OBJECT-TYPE
        SYNTAX DateAndTime
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The date and time that TLV 153 UsOfdmaConfigResponse was received from this RPD."
        DEFVAL { '0000000000000000'h }
        ::= { cerRpdUsOfdmaPreambleResponseGroup 10 }

