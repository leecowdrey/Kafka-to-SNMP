-- RPD DEPI Channel Selector to Channel IfIndex Table
--------------------------------------------------------------------------------------------------

cerRpdChanSelectorTable OBJECT-TYPE
    SYNTAX SEQUENCE OF CerRpdChanSelectorEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table contains an entry for each PHY channel on each RPD.
         This could include channels configured on a different core."
    ::= { cerRphyMib 29 }

cerRpdChanSelectorEntry OBJECT-TYPE
    SYNTAX CerRpdChanSelectorEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "An entry for one PHY channel."
    INDEX { cerRpdChanSelectorRpdIndex, 
            cerRpdChanSelectorValue, 
            cerRpdChanSelectorDataPwId }
    ::= {cerRpdChanSelectorTable 1}

CerRpdChanSelectorEntry ::= SEQUENCE {
    cerRpdChanSelectorRpdIndex            CerRpdIndex,
    cerRpdChanSelectorValue               Unsigned32,
    cerRpdChanSelectorDepiChanId          Unsigned32,
    cerRpdChanSelectorDataPwId            Unsigned32,
    cerRpdChanSelectorPhyIfIndex          InterfaceIndexOrZero,
    cerRpdChanSelectorMacIfIndex          InterfaceIndexOrZero,
    cerRpdChanSelectorPwSubType           PwSubtype
}

cerRpdChanSelectorRpdIndex OBJECT-TYPE
    SYNTAX CerRpdIndex
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The index of the RPD."
    ::= { cerRpdChanSelectorEntry 1 }

cerRpdChanSelectorValue OBJECT-TYPE
    SYNTAX Unsigned32
    MAX-ACCESS   not-accessible
    STATUS current
    DESCRIPTION
        "DEPI channel selector for a channel on the RPD. This 24 bit number
         has the RPD RF port number in the upper 8 bits, the channel type in
         the middle 8 bits, and the channel ID in low 8 bits."
    REFERENCE
        "DEPI Table 9"
    ::= { cerRpdChanSelectorEntry 2 }

cerRpdChanSelectorDepiChanId OBJECT-TYPE
    SYNTAX Unsigned32 (0..255)
    MAX-ACCESS   read-only
    STATUS current
    DESCRIPTION
        "DEPI channel ID associated with this RPD channel selector.
         Zero means that this channel selector channel type does not
         use a DEPI channel ID (only DOCSIS DS channels have a channel ID)."
    ::= { cerRpdChanSelectorEntry 3 }

cerRpdChanSelectorDataPwId OBJECT-TYPE
    SYNTAX Unsigned32
    MAX-ACCESS   not-accessible
    STATUS current
    DESCRIPTION
        "Rpd Pseudowire ID of DEPI session carrying this channel."
    ::= { cerRpdChanSelectorEntry 4 }

cerRpdChanSelectorPhyIfIndex OBJECT-TYPE
    SYNTAX InterfaceIndexOrZero 
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The E6000 RPD IfIndex of this PHY channel. Zero if the channel
         is configured by a different core."
    ::= { cerRpdChanSelectorEntry 5 }

cerRpdChanSelectorMacIfIndex OBJECT-TYPE
    SYNTAX InterfaceIndexOrZero 
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The E6000 IfIndex of the MAC channel associated with this RPD channel.
    Zero if the channel is configured by a different core."
    ::= { cerRpdChanSelectorEntry 6 }

cerRpdChanSelectorPwSubType OBJECT-TYPE
    SYNTAX          PwSubtype
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "The Session subtype for this PW." 
    ::= { cerRpdChanSelectorEntry 7 }


