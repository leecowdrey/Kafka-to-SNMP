-- ---------------------------------------------------------------------
-- The CMTS Upstream Triggered Spectrum Capture Config Table
-- ---------------------------------------------------------------------
cerPnmCmtsUtscCfgTable OBJECT-TYPE
     SYNTAX      SEQUENCE OF CerPnmCmtsUtscCfgEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
        "This table provides configuration functionality for the Core
         Upstream Triggered Spectrum Capture function."
     ::= { cerRphyMib 84 }

cerPnmCmtsUtscCfgEntry OBJECT-TYPE
     SYNTAX      CerPnmCmtsUtscCfgEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
         "The conceptual row of the cerPnmCmtsUtscCfgTable.
          The conceptual rows does not persist across device resets.
          A separate entry exists for every Upstream RF Port
          which supports the capability.
          This table supports the creation and deletion of at least one
          instance."
     INDEX { ifIndex,
             cerPnmCmtsUtscCfgIndex }
     ::= { cerPnmCmtsUtscCfgTable 1 }

CerPnmCmtsUtscCfgEntry ::= SEQUENCE {
    cerPnmCmtsUtscCfgIndex                  Unsigned32,
    cerPnmCmtsUtscCfgStaticPwDestIpAddrType InetAddressType,
    cerPnmCmtsUtscCfgStaticPwDestIpAddr     InetAddressIPv4or6,
    cerPnmCmtsUtscCfgStaticPwSessionId      Unsigned32,
    cerPnmCmtsUtscCfgStaticPwSrcIpAddr      InetAddressIPv4or6,
    cerPnmCmtsUtscCfgStaticPwSrcEnetPort    Unsigned32
}

cerPnmCmtsUtscCfgIndex OBJECT-TYPE
     SYNTAX      Unsigned32 (0..4294967295)
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
        "This key attribute is a unique identifier for a particular set
         of configuration settings. If the CORE does not support
         multiple Upstream Triggered Spectrum Sample Capture
         configuration settings, the CORE MUST only support a value of 1
         for this attribute and reject any attempt to configure it to a
         value other than 1."
     ::= { cerPnmCmtsUtscCfgEntry 1 }

cerPnmCmtsUtscCfgStaticPwDestIpAddrType OBJECT-TYPE
     SYNTAX      InetAddressType
     MAX-ACCESS  read-write
     STATUS      current
     DESCRIPTION
        "This attribute is the address type of the static psuedowires
         used between the RPD and the PNM server."
     DEFVAL { ipv6 }
     ::= { cerPnmCmtsUtscCfgEntry 2 }

cerPnmCmtsUtscCfgStaticPwDestIpAddr OBJECT-TYPE
     SYNTAX      InetAddressIPv4or6
     MAX-ACCESS  read-write
     STATUS      current
     DESCRIPTION
        "This attribute is the IP address of the PNM server that is
         the destination for all PNM UEPI packets from the RPD."
     DEFVAL { '00000000000000000000000000000000'H }
     ::= { cerPnmCmtsUtscCfgEntry 3 }

cerPnmCmtsUtscCfgStaticPwSessionId OBJECT-TYPE
     SYNTAX      Unsigned32
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION
        "This attribute shows the sessionId of the static psuedowire
         used for the PNM UEPI packets from the RPD."
     DEFVAL { 0 }
     ::= { cerPnmCmtsUtscCfgEntry 4 }

cerPnmCmtsUtscCfgStaticPwSrcIpAddr OBJECT-TYPE
     SYNTAX      InetAddressIPv4or6
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION
        "This attribute shows the source IP address used for the
         UEPI PNM packets from the RPD."
     DEFVAL { '00000000000000000000000000000000'H }
     ::= { cerPnmCmtsUtscCfgEntry 5 }

cerPnmCmtsUtscCfgStaticPwSrcEnetPort OBJECT-TYPE
     SYNTAX      Unsigned32 (0..1)
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION
        "This attribute shows the ethernet port associated 
         with the source IP address used for the
         UEPI PNM packets from the RPD."
     DEFVAL { 0 }
     ::= { cerPnmCmtsUtscCfgEntry 6 }

-- ---------------------------------------------------------------------
-- The RPD Aux Core Config Table
-- ---------------------------------------------------------------------     
cerRpdAuxCoreTable OBJECT-TYPE
    SYNTAX SEQUENCE OF CerRpdAuxCoreEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table contains the IP addresses of AUX cores to be sent in TLV 88 to a specific RPD.
          A row in this table for an RPD supersedes use of the global AUX cores in the cerRphyAuxCoreTable
         for that RPD."
    ::= { cerRphyMib 86 }

cerRpdAuxCoreEntry OBJECT-TYPE
    SYNTAX CerRpdAuxCoreEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "An entry for the table."
    INDEX { cerRpdAuxCoreRpdIndex, cerRpdAuxCoreAddrType, cerRpdAuxCoreIpAddr }
    ::= {cerRpdAuxCoreTable 1}

CerRpdAuxCoreEntry ::= SEQUENCE {
    cerRpdAuxCoreRpdIndex           CerRpdIndex,
    cerRpdAuxCoreAddrType           InetAddressType,
    cerRpdAuxCoreIpAddr             InetAddressIPv4or6,
    cerRpdAuxCoreDescription        DisplayString,
    cerRpdAuxCoreRowStatus          RowStatus
}

cerRpdAuxCoreRpdIndex OBJECT-TYPE
    SYNTAX CerRpdIndex
    MAX-ACCESS   not-accessible
    STATUS current
    DESCRIPTION
        "Index of the RPD using this Aux Core."
    ::= {cerRpdAuxCoreEntry 1}

cerRpdAuxCoreAddrType OBJECT-TYPE
    SYNTAX InetAddressType
    MAX-ACCESS   not-accessible
    STATUS current
    DESCRIPTION
        "Type of the AUX Core IP address; only IPv4 and IPv6 are allowed."
    ::= {cerRpdAuxCoreEntry 2}

cerRpdAuxCoreIpAddr OBJECT-TYPE
    SYNTAX InetAddressIPv4or6
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The IP address of this AUX core. Only length 4 for IPv4 and length 16 for
         IPv6 are allowed."
    ::= {cerRpdAuxCoreEntry 3}

cerRpdAuxCoreDescription OBJECT-TYPE
    SYNTAX       DisplayString (SIZE(0..32))
    MAX-ACCESS   read-create
    STATUS       current
    DESCRIPTION
        "The description of the AUX core."
    DEFVAL { "" }
    ::= {cerRpdAuxCoreEntry 4}

cerRpdAuxCoreRowStatus   OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The RowStatus for controlling entries in this table. 
         Allowed values are createAndGo, destroy, and active."
    ::= { cerRpdAuxCoreEntry 5 }
END
