cerL2Tpv3TunnelInfoTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF CerL2Tpv3TunnelInfoEntry 
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION
        "This table provides information about the L2TPv3 Tunnels on
         this core and, if reported by RPDs, other cores attached to this
         cores RPDs."
    ::= { cerRphyMib 24 }

cerL2Tpv3TunnelInfoEntry OBJECT-TYPE
    SYNTAX          CerL2Tpv3TunnelInfoEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION
        "A conceptual row for this table.
        Entries in this table are created when an L2Tpv3 Session 
        becomes active."
    INDEX  { cerL2Tpv3TunnelInfoRpdIndex, cerL2Tpv3TunnelInfoCoreCCID } 
    ::= { cerL2Tpv3TunnelInfoTable 1 }

CerL2Tpv3TunnelInfoEntry ::= SEQUENCE {
        cerL2Tpv3TunnelInfoRpdIndex          CerRpdIndex,
        cerL2Tpv3TunnelInfoCoreCCID          Unsigned32,
        cerL2Tpv3TunnelInfoRpdCCID           Unsigned32,
        cerL2Tpv3TunnelInfoIpAddrType        InetAddressType,
        cerL2Tpv3TunnelInfoRpdLcceIpAddr     InetAddressIPv4or6,
        cerL2Tpv3TunnelInfoUnknownPwPkts     Counter64,
        cerL2Tpv3TunnelInfoRpdUnknownPwPkts  Counter64,
        cerL2Tpv3TunnelInfoRpdEnetPortIndex  Unsigned32,
        cerL2Tpv3TunnelInfoRpdEnetPortGcp    TruthValue
}

cerL2Tpv3TunnelInfoRpdIndex OBJECT-TYPE
    SYNTAX CerRpdIndex
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This object represents the RPD at the far end of the tunnel."
    ::= { cerL2Tpv3TunnelInfoEntry 1 }

cerL2Tpv3TunnelInfoCoreCCID OBJECT-TYPE
    SYNTAX          Unsigned32 (1..4294967295)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION
        "The E6000 L2TPv3 CCID of the tunnel." 
